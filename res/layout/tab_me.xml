<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <include layout="@layout/top_me" />
    <include layout="@layout/me_privacy_settings"/>
     <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/login_divider"
        />
    <include layout="@layout/me_message_settings"/>
     <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/login_divider"
        />
     <include layout="@layout/me_update_settings"/>
      <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/login_divider"
        />
     <include layout="@layout/me_fun_settings"/>
      <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/login_divider"
        />
      <include layout="@layout/me_update_settings"/>
      <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/login_divider"
        />
     <include layout="@layout/me_about_settings"/>
      <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/login_divider"
        />
    
    <Button
        android:id="@+id/quit_login_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/layout_margin"
        android:layout_marginLeft="@dimen/me_settings_padding"
        android:layout_marginRight="@dimen/me_settings_padding"
        android:text="@string/quit_login"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/partner_commit_ic"
        android:textColor="@color/white"
/>

</LinearLayout>